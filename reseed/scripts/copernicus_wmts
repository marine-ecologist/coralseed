  new Cesium.WebMapTileServiceImageryProvider({
    url: "https://wmts.marine.copernicus.eu/teroWmts",
    layer: "GLOBAL_ANALYSISFORECAST_PHY_001_024/cmems_mod_glo_phy-cur_anfc_0.083deg_P1D-m_202406/vo",
    //layer: "GLOBAL_ANALYSISFORECAST_PHY_001_024/cmems_mod_glo_phy-cur_anfc_0.083deg_P1D-m_202406/uo",
    style: "cmap:viridis",
    format: "image/png",
    tileMatrixSetID: "EPSG:4326",
    maximumLevel: 10,
    tilingScheme: new Cesium.GeographicTilingScheme(),
  })
);


